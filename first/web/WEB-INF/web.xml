<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>suck</servlet-name>
        <servlet-class>cool.start</servlet-class>
        
    </servlet>
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>cool.searchSelector</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ConfirmationServlet</servlet-name>
        <servlet-class>cool.ConfirmationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>objJsp</servlet-name>
        <jsp-file>obj.jsp</jsp-file>
        <init-param>
            <param-name>webxmlparam</param-name>
            <param-value>webb</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>start</servlet-name>
        <servlet-class>cool.formHandlerExt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>suck</servlet-name>
        <url-pattern>/start</url-pattern>
        
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>objJsp</servlet-name>
        <url-pattern>/obj.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/searchSelector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>start</servlet-name>
        <url-pattern>/formhandlerservletExt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmationServlet</servlet-name>
        <url-pattern>/ConfirmationServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>searchEngineSelector.html</welcome-file>
    </welcome-file-list>
</web-app>